                    <form action="#" class="form form--consultation">                        

                        <div class="row">
                            <div class="mv_24 tb_8 ds_8 field-outer">
                                <input name="your-name" class="field--text" placeholder="Ваше имя" type="text">
                            </div>
                            <div class="mv_24 tb_8 ds_8 field-outer">
                                <input name="your-name" class="field--text" placeholder="Ваше имя" type="phone">
                            </div>
                            <div class="mv_24 tb_8 ds_8 field-outer">
                                <a href="#" class="btn btn-submit">Отправить заявку</a>
                            </div>
                        </div>

                    </form>
            </div>
        </div>

        <div class="row">
            <div class="mv_24 tb_24 ds_18 ds_offset_3">
                <div class="form__notice form__notice--white">Нам доверяют больше <b>500 клиентов</b></div>
            </div>                 
        </div>        

 
    </div>

</section>




<section class="section section-news">
    <div class="container">

        <div class="row">
            <div class="mv_24">
                <h2 class="title">Новости отрасли</h2>
            </div>
        </div>


        <div class="row  ">
            
           <div class="mv_24 news--card ">
                <div class="row ">


                {{#each news}}

                   <div class="mv_24 mv_offset_0 tb_12 tb_offset_0 ds_6 news__item-outer--{{@index}}">
                       <div class="news__item">
                   
                           <a href="{{item-link}}" class="news__item-image">
                               <img src="uploads/news/{{item-image}}" alt="{{item-title}}">
                           </a>
                   
                           <div class="news__item-content">
                                <div class="news__item-content-inner">
                                    <div class="news__item-title">
                                        <a href="#">{{capitals item-title}} {{item-title}}</a>
                                    </div>
                                    <time class="news__item-date" datetime="{{item-datetime}}">{{item-date}}</time>
                                    <div class="news__item-text">{{item-text}}</div>
                                </div>

                                <div class="news__item-button">
                                   <a href="{{item-link}}" class="btn btn-readmore">Читать дальше</a>
                                </div>
                           </div>
                       </div>
                   </div>
                {{/each}}


                </div>


           </div>

        </div>


    </div>
</section>




{{> footer}}



